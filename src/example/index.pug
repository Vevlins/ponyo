                $('#notice').click(function(){
                    $('#hide').addClass("fadeIn");
                    $('#hide').show();
                    setTimeout(function(){
                        $('#hide').removeClass("fadeIn");
                    },500);
                })
                $('#hide').click(function(){
                    console.log(111);
                    $('#hide').addClass("fadeOut");
                    setTimeout(()=>{
                        $('#hide').hide();
                        $('#hide').removeClass("fadeOut");
                    },500)
                })
            })
